{
  "name": "@shadowspawn/forest-arborist",
  "version": "0.2.11",
  "description": "Forest Arborist. A loosely coupled repository manager inspired by Mercurial subrepositories, npm, and Git.",
  "private": false,
  "preferGlobal": true,
  "bin": {
    "fab": "./dist/command.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnRGee/forest-arborist.git"
  },
  "scripts": {
    "copy-up": "git checkout master && git merge develop && echo \"tip: run npm version patch, then npm publish\"",
    "copy-down": "git checkout develop && git merge master && npm --no-git-tag-version version patch",
    "lint": "./node_modules/eslint/bin/eslint.js command.js lib/** spec/**",
    "test": "jasmine",
    "prepublish:WIP": "tsc"
  },
  "author": "John Gee",
  "license": "ISC",
  "files": [
    "dist/command.js",
    "dist/lib/*"
  ],
  "dependencies": {
    "@types/chalk": "^0.4.31",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "jsonfile": "^2.4.0",
    "mute": "^2.0.6",
    "shell-quote": "^1.6.1",
    "tmp": "^0.0.31"
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.9.0",
    "jasmine-console-reporter": "^1.2.7",
    "typescript": "^2.2.1"
  }
}
