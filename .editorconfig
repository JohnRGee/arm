# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
# Using airbnb eslint style guide, two spaces
indent_size = 2
tab_width = 2

# Tab indentation (no size specified)
[{Makefile,makefile}]
indent_style = tab
